<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.ordertech.cloudsqlproxy</string>

    <key>ProgramArguments</key>
    <array>
      <string>/opt/homebrew/bin/cloud-sql-proxy</string>
      <string>smart-order-469705:me-central1:ordertech-db</string>
      <string>--unix-socket</string>
      <string>/Users/mosawi/.cloudsql</string>
    </array>

    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Users/mosawi/.cloudsql/proxy.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/mosawi/.cloudsql/proxy.err</string>
  </dict>
</plist>