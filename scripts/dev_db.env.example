# Copy to scripts/dev_db.env and fill in your values
# Project/instance
PROJECT_ID=smart-order-469705
REGION=europe-west1
# Either set INSTANCE_NAME (below) OR set CONNECTION_NAME directly
INSTANCE_NAME=smart-order-pg
# CONNECTION_NAME=smart-order-469705:europe-west1:smart-order-pg
PROXY_PORT=5432

# Database credentials
DB_USER={{DB_USER}}
DB_NAME={{DB_NAME}}
# Name of Secret Manager secret that stores the DB password
DB_PASSWORD_SECRET={{DB_PASSWORD_SECRET_NAME}}
# Alternatively, provide the full DATABASE_URL via Secret Manager and we will rewrite host:port to localhost
DB_URL_SECRET=DATABASE_URL

